{# templates/category/edit.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}Edit Category{% endblock %}

{% block body %}
    <h1>Edit Category</h1>

    {{ form_start(form) }}
    {{ form_row(form.title) }}
    {{ form_row(form.webTitle) }}
    {{ form_row(form.parent) }}
    {{ form_row(form.level) }}
    {{ form_row(form._token) }}

    <button type="submit">Save Changes</button>
    {{ form_end(form) }}
{% endblock %}