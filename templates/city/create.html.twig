{# templates/city/create.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}Create City{% endblock %}

{% block body %}
    <h1>Create City</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.countryCode) }}
    {{ form_row(form.district) }}
    {{ form_row(form._token) }}

    <button type="submit">Create City</button>
    {{ form_end(form) }}
{% endblock %}
